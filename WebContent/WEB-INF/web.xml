<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Advanced2C</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>


  <servlet>
  	<servlet-name>register</servlet-name>
  	<servlet-class>A2C.register</servlet-class>
  	</servlet>
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>login_check1</servlet-name>
  	<servlet-class>A2C.login_check1</servlet-class>
  	</servlet>
  <servlet-mapping>
  	<servlet-name>login_check1</servlet-name>
  	<url-pattern>/login_check1</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>logout</servlet-name>
  	<servlet-class>A2C.logout</servlet-class>
  	</servlet>
  <servlet-mapping>
  	<servlet-name>logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>MyDBAccess</servlet-name>
  	<servlet-class>A2C.MyDBAccess</servlet-class>
  	</servlet>
  <servlet-mapping>
  	<servlet-name>MyDBAccess</servlet-name>
  	<url-pattern>/MyDBAccess</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>application_shortcuts</servlet-name>
  	<servlet-class>A2C.application_shortcuts</servlet-class>
  	</servlet>
  <servlet-mapping>
  	<servlet-name>application_shortcuts</servlet-name>
  	<url-pattern>/application_shortcuts</url-pattern>
  </servlet-mapping>
  
  
</web-app>